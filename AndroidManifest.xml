<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.quizapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/aicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splashactivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>

        <activity android:name=".QuizActivity"></activity>
        <activity android:name=".result"></activity>
        <activity android:name=".quizactivity2"></activity>
        <activity android:name=".result2"></activity>


    </application>

</manifest>